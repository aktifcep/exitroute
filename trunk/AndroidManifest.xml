<?xml version="1.0" encoding="UTF-8"?>
<manifest package="com.naholyr.android.games.offroad"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:versionName="0.1" android:sharedUserId="@string/shared_user_id"
	android:sharedUserLabel="@string/shared_user_id" android:versionCode="1">
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:label="@string/app_name" android:name=".activity.Main">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".activity.Game"
			android:screenOrientation="portrait" android:configChanges="keyboardHidden|orientation" />
	</application>
	<uses-sdk android:minSdkVersion="3"></uses-sdk>
</manifest>