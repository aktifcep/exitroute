<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="fill_parent" android:layout_width="fill_parent"
	android:orientation="vertical">
	<LinearLayout android:layout_height="wrap_content"
		android:layout_width="fill_parent" android:orientation="horizontal">
		<CheckBox android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Une checkbox bidon" android:id="@+id/checkbox"/>
	</LinearLayout>
	<FrameLayout android:id="@+id/GameFrame"
		android:layout_height="fill_parent" android:layout_width="fill_parent" android:foregroundGravity="top|left"/>
</LinearLayout>
